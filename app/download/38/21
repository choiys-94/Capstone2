	//게시판 이미지 리사이즈
	try{resize_image();}catch(e){}

	//신규메뉴 오픈 스크립트
	try{jf_menuUIOpenInit();}catch(e){}

	//페이지 타이틀
	getPageTitle();

	
	//서브메뉴 리사이즈 333
