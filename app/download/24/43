<script>
$(document).ready(function() {

	$(".box").eq(0).css("display","block");
	$(".tab02 > li").click(function() {

		$(".tab02 > li").removeClass("on");

		var curr_idx = $(".tab02 > li").index(this);
		$(".tab02 > li").eq(curr_idx).addClass("on");
		$(".box").hide();
		$(".box").eq(curr_idx).show();

		return false;
	});

});


