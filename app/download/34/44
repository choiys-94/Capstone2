$(document).ready(function() {
	//통합검색 입력필드 배경 토글
	$(".campusmap_wrap .campusmap_search .srch .keyword").focus(function (){
		$(this).removeClass("bg");
	}).blur(function (){
	
		if($(this).val() == ""){
			$(this).addClass("bg");
		}
	});
	
	//건물명 툴팁
	//$("#map_main").attr("alt", "");
