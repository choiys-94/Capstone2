//		return false;
//	});
	
	//하단 장소 클릭시 이미지 변경
	$(".list02 > li > a").click(function() {
		var path = "/mbs/chosun/jsp/university/map_img/";
		$("#map_main").attr("src", path + $(this).parent().attr("class") + ".jpg" );
		return false;
	});

	
});

function jf_view(univSeq){
	$("#empty_box").hide();
	
	if(document.getElementById("nowShowId").value == ""){
		$("#"+univSeq).show();
	}else{
		$("#"+document.getElementById("nowShowId").value).hide();
		$("#"+univSeq).show();
	}
	document.getElementById("nowShowId").value=univSeq;
	
}

function getURLParameter(url, name) {
	return (RegExp(name + '=' + '(.+?)(&|$)').exec(url)||[,null])[1];
}
</script>

<input type="hidden" id="nowShowId"/>
