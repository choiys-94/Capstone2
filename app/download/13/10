	function sendTwitter(sTitle,sUrl){
		var shref ="http://twitter.com/home?status="+encodeURIComponent(sTitle)+" "+encodeURIComponent(sUrl);
		var sWindow=window.open(shref);
		if (sWindow){
			sWindow.focus();
		}
	}

	//페이스북
	function sendFacebook(sTitle,sUrl){
		var shref ="http://www.facebook.com/share.php?u="+encodeURIComponent(sUrl)+"&t="+encodeURIComponent(sTitle);
		var sWindow=window.open(shref);
		if (sWindow){
			sWindow.focus();
		}
	}
	function depart_pass_pop(){
		var date1 = "1531818000";
		var date2 = "1544037956";
		if(date2 > date1){
			window.open("/popup_main/popup_list.php","","width=1020,height=900,scrollbars=yes")
		}else{
			alert("금일 오후 6시부터 확인 가능합니다.");
		}
	}
