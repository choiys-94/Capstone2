<script language="JavaScript">
 
function popup_setCookie( name, value, expiredays ) 
    { 
        var todayDate = new Date(); 
        todayDate.setDate( todayDate.getDate() + expiredays ); 
        document.cookie = name + "=" + escape( value ) + "; path=/; expires=" + todayDate.toGMTString() + ";" 
        } 
 
function popup_closeWin() 
{ 
        if ( document.forms[0].popup.checked ) 
                popup_setCookie( "popup_181109", "done" , 1); // 1=하룻동안 공지창 열지 않음 
        self.close(); 
} 
 
 
	function win_link(url_link) { 
		window.opener.location=url_link; 
		//opener.win_close();
		self.close();
 
	}

</script>



