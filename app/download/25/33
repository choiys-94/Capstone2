	function pre_confirm_chk(){

		var returnUrl = "/user/boardList.do";
 
		returnUrl +="?boardId=8467"
				  +	"&siteId=sukang"
				  +	"&page=1" 
				  +	"&command=write";
 
		$.ajax({
			type:"post",
			url:"/join/preConfirmChk.jsp", 
			async: false, 
			cache:false, 
			data:{
				"returnUrl":returnUrl, 
			},
			success:function(r){
				r = r.replace( /(^\s*)|(\s*$)/g, "" );

				if(r == "false"){
					alert('본인인증 후 이용가능합니다.');
					var siteUrl = "/join/login2.jsp?siteId=sukang";
					location.href = siteUrl;
				}else{ 
					form_list_submit(); 
				} 
			},
			error:function(){
				alert("error!");
			} 
		});
	}
