<script type="text/javascript">
/* <![CDATA[ */
function tabBoardChange_7390(no){
	var obj;
	var boardCnt = 3;
	for (var i=1;i<=boardCnt; i++)
	{	
		obj = document.getElementById("tabList_7390_"+i);
		if (obj)
		{
			if (no==i)
			{
				obj.style.display = "";
				$("#tabBoardList_7390 > ul > li > a > span").eq(i-1).addClass("over");
			} else {
				obj.style.display = "none";
				$("#tabBoardList_7390 > ul > li > a > span").eq(i-1).removeClass("over");
			}
		} else {
			break;
		}
	}
}


















function jf_view_(site_id, board_id, board_seq,secret_yn,secret_num,sboard_id){
	var strPath  = '/user/boardList.do';
		strPath += '?siteId=sukang&boardId='+board_id;
		strPath += '&command=view&page=1&boardSeq=' + board_seq; 
		strPath += '&boardType='; 
		strPath += '&parent=%2Fuser%2FindexSub.do%3FframePath%3Dunknownboard%26siteId%3Dsukang'; 
  location.href =strPath;
   return false;
}




	













