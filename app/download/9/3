			<script>
function agree_submit(form) {
	var form = document.agreeform;

		if(!form.private.checked) {
            alert("개인정보취급방침에 동의하셔야 합니다.");
            form.private.focus();
           return false;
        }

		if(!form.agree.checked) {
            alert("회원 약관에 동의하셔야 합니다.");
            form.agree.focus();
           return false;
        }

		return true;
 }
</script>	

